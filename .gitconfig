[user]
    name = pawurb
    email = p.urbanek89@gmail.com
[alias]
    ch = checkout
    re = rebase
[push]
    default = tracking
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    excludesfile = ~/.gitignore_global
    editor = vim
    filemode = false
